@model IEnumerable<Dogevents.Core.Domain.Event>

<table>
    <thead>
    <th>Id</th>
    <th>Title</th>
    <th>Date</th>
    <th>Categories</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>from: @item.DateStart, to: @item.DateEnd</td>
                <td>@string.Join(",", @item.Categories)</td>
            </tr>
        }
    </tbody>
</table>